{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "love",
    "eslint-config-prettier"
  ],
  "overrides": [{
    "files": ["**/*.ts"]
  }],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["./tsconfig.json"]
  },
  "rules": {
    "@typescript-eslint/semi": 0,
    "@typescript-eslint/comma-dangle": 0
  },
  "ignorePatterns": ["build"]
}
